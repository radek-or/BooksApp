{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": "eslint:recommended",
	"parserOptions": {
		"ecmaVersion": 2018, // Zmiana na 2018
		"sourceType": "module"
	},
	"rules": {
		"indent": ["error", "tab"], // Zmiana na tabulacjÄ™
		"linebreak-style": ["off"],
		"quotes": ["error", "double", { "allowTemplateLiterals": true }],
		"semi": ["error", "always"],
		"no-console": ["off"]
	},
	"globals": {
		"Handlebars": true,
		"dataSource": true,
		"utils": true
	}
}
